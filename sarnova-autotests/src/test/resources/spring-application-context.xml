<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
    http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.2.xsd
	">

    <context:component-scan base-package="com.sarnova.hybris.backoffice.pages"/>
    <context:component-scan base-package="com.sarnova.hybris.hac.pages"/>
    <context:component-scan base-package="com.sarnova.hybris.import_cockpit.pages"/>
    <context:component-scan base-package="com.sarnova.pay_fabric.pages"/>
    <context:component-scan base-package="com.sarnova.storefront.pages"/>
    <context:component-scan base-package="com.sarnova.storefront.page_blocks"/>
    <context:component-scan base-package="com.sarnova.helpers"/>
    <context:component-scan base-package="com.sarnova.storefront.managers"/>

    <import resource="classpath:spring-credentials-config-context.xml" />

</beans>